<Window x:Class="AutoChapsMp4.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoChapsMp4"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="300" Width="300">
	<Grid>
		<Label x:Name="label" Content="Mp4 Chaps Location:" HorizontalAlignment="Left" Margin="10,28,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="textBox" HorizontalAlignment="Left" Height="33" Margin="10,54,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="235"/>
		<Button x:Name="button" Content="Browse" HorizontalAlignment="Left" Margin="10,92,0,0" VerticalAlignment="Top" Width="75"/>
		<Label x:Name="label_Copy" Content="Tag Lib Sharp Location" HorizontalAlignment="Left" Margin="10,117,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="33" Margin="10,148,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="235"/>
		<Button x:Name="button_Copy" Content="Browse" HorizontalAlignment="Left" Margin="10,186,0,0" VerticalAlignment="Top" Width="75"/>

	</Grid>
</Window>
